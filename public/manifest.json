{
  "manifest_version": 3,
  "name": "QR Code Generator",
  "version": "1.0",
  "description": "Generate QR codes from any webpage or selected text",
  "permissions": ["activeTab", "contextMenus"],
  "action": {
    "default_popup": "index.html"
  },

  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
